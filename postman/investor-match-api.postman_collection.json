  "item": [
    {
      "name": "Contacts (v1)",
      "item": [
        {
          "name": "Create Contact",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" },
              { "key": "accept", "value": "application/json" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"full_name\": \"Alicia Founder\",\n  \"contact_type\": \"founder\",\n  \"headline\": \"Building embedded fintech rails\",\n  \"email\": \"alicia@example.com\",\n  \"location_country\": \"USA\",\n  \"location_city\": \"New York\",\n  \"skills\": [\"javascript\", \"product_management\"],\n  \"industries\": [\"fintech\", \"saas\"],\n  \"verticals\": [\"b2b\"],\n  \"funding_stages\": [\"seed\"],\n  \"job_to_be_done\": [\"fundraising\"],\n  \"roles\": [\"ceo\"],\n  \"experiences\": [\n    {\n      \"company\": \"FinTech Labs\",\n      \"role\": \"CEO\",\n      \"start_year\": 2022,\n      \"end_year\": null\n    }\n  ]\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/v1/contacts",
              "host": ["{{baseUrl}}"],
              "path": ["v1", "contacts"]
            }
          }
        },
        {
          "name": "Get Contact",
          "request": {
            "method": "GET",
            "header": [{ "key": "accept", "value": "application/json" }],
            "url": {
              "raw": "{{baseUrl}}/v1/contacts/{{contactId}}",
              "host": ["{{baseUrl}}"],
              "path": ["v1", "contacts", "{{contactId}}"]
            }
          }
        },
        {
          "name": "Update Contact",
          "request": {
            "method": "PATCH",
            "header": [
              { "key": "Content-Type", "value": "application/json" },
              { "key": "accept", "value": "application/json" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"headline\": \"Now leading GTM at FinTech Labs\",\n  \"skills\": [\"javascript\", \"product_management\", \"sales\"]\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/v1/contacts/{{contactId}}",
              "host": ["{{baseUrl}}"],
              "path": ["v1", "contacts", "{{contactId}}"]
            }
          }
        },
        {
          "name": "Delete Contact",
          "request": {
            "method": "DELETE",
            "header": [{ "key": "accept", "value": "application/json" }],
            "url": {
              "raw": "{{baseUrl}}/v1/contacts/{{contactId}}",
              "host": ["{{baseUrl}}"],
              "path": ["v1", "contacts", "{{contactId}}"]
            }
          }
        }
      ]
    },
    {
      "name": "Contact Queries & Insights",
      "item": [
        {
          "name": "Filter Contacts",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" },
              { "key": "accept", "value": "application/json" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"contact_type\": \"investor\",\n  \"industries\": [\"fintech\"],\n  \"skills\": [\"product_management\"],\n  \"match_mode\": \"any\",\n  \"limit\": 25\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/contacts/filter",
              "host": ["{{baseUrl}}"],
              "path": ["contacts", "filter"]
            }
          }
        },
        {
          "name": "Contact Matches (v1)",
          "request": {
            "method": "GET",
            "header": [{ "key": "accept", "value": "application/json" }],
            "url": {
              "raw": "{{baseUrl}}/v1/contacts/{{contactId}}/matches?type={{matchType}}&limit=10",
              "host": ["{{baseUrl}}"],
              "path": ["v1", "contacts", "{{contactId}}", "matches"],
              "query": [
                { "key": "type", "value": "{{matchType}}" },
                { "key": "limit", "value": "10" }
              ]
            }
          }
        },
        {
          "name": "Campaign Analysis (legacy)",
          "request": {
            "method": "GET",
            "header": [{ "key": "accept", "value": "application/json" }],
            "url": {
              "raw": "{{baseUrl}}/contacts/{{contactId}}/campaign-analysis",
              "host": ["{{baseUrl}}"],
              "path": ["contacts", "{{contactId}}", "campaign-analysis"],
              "query": [
                { "key": "target_type", "value": "{{matchType}}", "disabled": true }
              ]
            }
          }
        },
        {
          "name": "Campaign Matches (legacy)",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" },
              { "key": "accept", "value": "application/json" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"target_type\": \"investor\",\n  \"limit\": 10\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/contacts/{{contactId}}/campaign-matches",
              "host": ["{{baseUrl}}"],
              "path": ["contacts", "{{contactId}}", "campaign-matches"]
            }
          }
        }
      ]
    },
    {
      "name": "Introductions",
