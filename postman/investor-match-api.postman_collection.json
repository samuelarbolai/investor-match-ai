              "raw": "{\n  \"full_name\": \"Alicia Founder\",\n  \"contact_type\": \"founder\",\n  \"headline\": \"Building embedded fintech rails\",\n  \"email\": \"alicia@example.com\",\n  \"location_country\": \"USA\",\n  \"location_city\": \"New York\",\n  \"current_company\": \"Atlas Labs\",\n  \"current_role\": \"Co-founder & CEO\",\n  \"job_to_be_done\": [\n    \"raise_seed\"\n  ],\n  \"skills\": [\n    \"javascript\",\n    \"product_management\"\n  ],\n  \"industries\": [\n    \"fintech\",\n    \"saas\"\n  ],\n  \"verticals\": [\n    \"payments\"\n  ],\n  \"raised_capital_range_ids\": [\n    \"less_than_500k_usd\"\n  ],\n  \"stage_counts\": {\n    \"prospect\": 3,\n    \"lead\": 1,\n    \"to-meet\": 0,\n    \"met\": 0,\n    \"not-in-campaign\": 0,\n    \"disqualified\": 0\n  },\n  \"experiences\": [\n    {\n      \"company_name\": \"Atlas Labs\",\n      \"company_id\": \"atlas_labs\",\n      \"role\": \"CEO\",\n      \"seniority\": \"executive\",\n      \"start_date\": \"2023-01\",\n      \"end_date\": null,\n      \"current\": true,\n      \"description\": \"Building embedded payments APIs\",\n      \"location_city\": \"New York\",\n      \"location_country\": \"USA\"\n    }\n  ],\n  \"distribution_capabilities\": [\n    {\n      \"distribution_type\": \"SocialMedia\",\n      \"label\": \"Twitter audience 50k\",\n      \"size_score\": 0.8,\n      \"engagement_score\": 0.7,\n      \"quality_score\": 0.6,\n      \"source_url\": \"https://twitter.com/alicia\"\n    }\n  ],\n  \"target_criteria\": [\n    {\n      \"dimension\": \"Industry\",\n      \"operator\": \"in\",\n      \"value\": [\n        \"Fintech\",\n        \"Payments\"\n      ]\n    },\n    {\n      \"dimension\": \"Location\",\n      \"operator\": \"in\",\n      \"value\": [\n        \"USA\",\n        \"Canada\"\n      ]\n    }\n  ],\n  \"companies\": [\n    {\n      \"name\": \"Atlas Labs\",\n      \"domain\": \"atlaslabs.com\"\n    }\n  ]\n}"
